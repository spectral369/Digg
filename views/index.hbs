		<!-- Work -->
		<article id="work">
			<h2 class="major">Bartender Area</h2>
			<div id="loginArea">
				<form method="post" action="javascript:void(0)" ;>
					<div class="fields">
						<div class="field half">
							<label for="username">Username</label>
							<input type="text" name="username" class="form-control" id="usernameLogin" minlength="3"
								oninput="all_lowercase()" required />
						</div>

						<div class="field half">
							<label for="password">Password</label>
							<input type="password" name="password" class="form-control" id="passwordLogin" minlength="6"
								oninput="passwordVa()" required />
						</div>

					</div>
					<ul class="actions">
						<li><input type="submit" id="login" value="Login" class="primary" /></li>
						<li><input type="button" id="toRegister" value="Register" class="primary" /></li>
					</ul>
					
				</form>
			</div>
			<div id="registerArea" hidden>
				<form method="post" action="javascript:void(0)" ;>
					<div class="fields">
						<div class="field half">
							<label for="username">Username</label>
							<input type="text" name="username" id="username" minlength="3" oninput="all_lowercase()" />
						</div>
						<div class="field half">
							<label for="namefull">Full Name</label>
							<input type="text" name="namefull" id="namefull" minlength="4"
								oninput="capitalize_fullname()" />
						</div>
						<div class="field half">
							<label for="password">Password</label>
							<input type="password" name="password" id="password" id="password" minlength="6"
								oninput="validatePassword();" required />
						</div>
						<div class="field half">
							<label for="password2">Re-type Password</label>
							<input type="password" name="password2" id="password2" id="password2" minlength="6"
								oninput="validatePassword();" required />
						</div>
					</div>
					<ul class="actions">
						<li><input type="submit" id="register" value="Register" class="primary" /></li>
						<li><input type="button" id="returnToLogin" value="Back" class="secondary" /></li>
					</ul>
					
				</form>
			</div>
